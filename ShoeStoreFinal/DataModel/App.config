<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework"
      type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
      requirePermission="false"/>
  </configSections>
  <connectionStrings>
    <add name="StoreDbConnexionString"
    connectionString="Data Source=.;Initial Catalog=FinalProject;Integrated Security=true"
    providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="DatabaseInitializerForType DataModel.StoreContext, DataModel"
        value="System.Data.Entity.DropCreateDatabaseAlways`1[[DataModel.StoreContext, DataModel]], EntityFramework" />
  </appSettings>

  
  <entityFramework>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>
</configuration>